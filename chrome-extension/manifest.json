{
	"manifest_version": 3,
	"name": "Your Extension Name",
	"version": "1.0",
	"permissions": ["activeTab", "scripting", "storage", "tabs", "contextMenus", "webNavigation", "notifications"],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["https://chatgpt.com/*"],
			"js": ["content.js"]
		}
	],

	"action": {},

	"web_accessible_resources": [
		{
			"resources": ["assets/*"],
			"matches": ["https://chatgpt.com/*"]
		}
	]
}
