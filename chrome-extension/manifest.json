{
	"manifest_version": 3,
	"name": "Your Extension Name",
	"version": "1.0",
	"permissions": ["storage", "tabs"],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],

	"action": {},

	"web_accessible_resources": [
		{
			"resources": ["assets/*"],
			"matches": ["<all_urls>"]
		}
	]
}
